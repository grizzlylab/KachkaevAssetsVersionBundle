{
	"name" : "grizzlylab/assets-version-bundle",
	"type" : "symfony-bundle",
	"description" : "Automates the process of updating assets version in Symfony 4/5 projects",
	"keywords" : [
		"assets",
		"assetic",
		"version",
		"autoincrement",
		"config",
		"console",
		"deploy"
	],
	"homepage" : "http://github.com/grizzlylab/KachkaevAssetsVersionBundle",
	"license" : "MIT",
	"authors" : [{
			"name" : "Alexander Kachkaev",
			"email" : "alexander@kachkaev.ru",
			"homepage" : "http://en.kachkaev.ru/"
		},
		{
			"name" : "Jean-Louis Pirson",
			"email" : "jl.pirson@grizzlylab.be",
			"homepage" : "https://www.grizzlylab.be"
		},
		{
			"name" : "Contributors",
			"homepage" : "https://github.com/grizzlylab/KachkaevAssetsVersionBundle/contributors"
		}
	],
	"require" : {
		"php" : "^7.4",
		"symfony/framework-bundle" : "^4.4|^5.1"
	},
	"require-dev" : {
		"symfony/filesystem" : "^4.4|^5.1",
		"symfony/finder" : "^4.4|^5.1",
		"phpunit/phpunit": "~9.4"
	},
    "scripts": {
        "phpunit": "phpunit -c phpunit.xml.dist; exit 0"
    },
	"suggest" : {
		"symfony/assetic-bundle" : "For combining and compressing assets"
	},
	"autoload" : {
        "psr-4": {
            "Kachkaev\\AssetsVersionBundle\\": ""
        }
	}
}
